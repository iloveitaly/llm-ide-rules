{
  "mcp": {
    "deepwiki": {
      "type": "sse",
      "url": "https://mcp.deepwiki.com/mcp",
      "enabled": true
    },
    "playwright": {
      "type": "local",
      "command": [
        "npx",
        "@playwright/mcp@latest",
        "--isolated",
        "--cap",
        "visual"
      ],
      "enabled": true
    },
    "chrome-mcp-server": {
      "type": "sse",
      "url": "http://127.0.0.1:12306/mcp",
      "enabled": true
    },
    "context7": {
      "type": "sse",
      "url": "https://mcp.context7.com/mcp",
      "enabled": true
    },
    "stripe": {
      "type": "sse",
      "url": "https://mcp.stripe.com",
      "enabled": true
    },
    "chrome-devtools": {
      "type": "local",
      "command": [
        "npx",
        "chrome-devtools-mcp@latest"
      ],
      "enabled": true
    },
    "terraform": {
      "type": "local",
      "command": [
        "docker",
        "run",
        "-i",
        "--rm",
        "hashicorp/terraform-mcp-server"
      ],
      "enabled": true
    }
  }
}