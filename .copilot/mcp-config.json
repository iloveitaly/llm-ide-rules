{
  "mcpServers": {
    "deepwiki": {
      "type": "http",
      "url": "https://mcp.deepwiki.com/mcp",
      "tools": [
        "*"
      ]
    },
    "playwright": {
      "type": "local",
      "command": "npx",
      "args": [
        "@playwright/mcp@latest",
        "--isolated",
        "--cap",
        "visual"
      ],
      "tools": [
        "*"
      ]
    },
    "chrome-mcp-server": {
      "type": "http",
      "url": "http://127.0.0.1:12306/mcp",
      "tools": [
        "*"
      ]
    },
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp",
      "tools": [
        "*"
      ]
    },
    "stripe": {
      "type": "http",
      "url": "https://mcp.stripe.com",
      "tools": [
        "*"
      ]
    },
    "chrome-devtools": {
      "type": "local",
      "command": "npx",
      "args": [
        "chrome-devtools-mcp@latest"
      ],
      "tools": [
        "*"
      ]
    },
    "terraform": {
      "type": "local",
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "hashicorp/terraform-mcp-server"
      ],
      "tools": [
        "*"
      ]
    }
  }
}